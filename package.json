{
  "name": "Trillo",
  "version": "1.0.0",
  "description": "Trillo App",
  "main": "index.js",
  "scripts": {
    "prefix:sass": "postcss --use autoprefixer -b 'last 10 version' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Malek Z",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.6.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "sass": "^1.23.0"
  }
}
